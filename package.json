{
  "name": "next-contentful-blog-template",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "postexport": "node utils/post-export.js",
    "type-check": "tsc",
    "format:write": "prettier \"**/*.{tsx,js,json,md,css}\" --write",
    "format:test": "prettier \"**/*.{tsx,js,json,md,css}\" --list-different",
    "lint": "eslint --fix . && echo 'Lint complete.'",
    "release": "standard-version && git push --follow-tags origin master",
    "populate:contentful": "contentful space migration --space-id <SPACE_ID> --management-token <TOKEN> utils/contentful.js --yes"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dsikorska/next-contentful-blog-template.git"
  },
  "keywords": [
    "nextjs",
    "javascript",
    "boilerplate",
    "contentful",
    "typescript"
  ],
  "author": "Dominika Sikorska",
  "license": "CHECK LICENSE UNDER LICENSE.md",
  "pre-commit": "pretty-quick --staged",
  "dependencies": {
    "contentful": "~9.1.4",
    "dotenv": "~10.0.0",
    "dotenv-webpack": "^7.0.3",
    "eslint-plugin-prettier": "~4.0.0",
    "express": "~4.17.1",
    "isomorphic-unfetch": "~3.1.0",
    "next": "~12.1.5",
    "react": "~18.0.0",
    "react-dom": "~18.0.0",
    "react-markdown": "~7.1.0",
    "sass": "^1.43.4",
    "serve": "~12.0.1"
  },
  "devDependencies": {
    "@types/node": "~16.11.6",
    "@types/react": "~17.0.33",
    "@types/react-dom": "~17.0.10",
    "@typescript-eslint/eslint-plugin": "~4.22.1",
    "@typescript-eslint/parser": "~4.22.1",
    "autoprefixer": "^10.4.4",
    "contentful-cli": "^1.12.19",
    "contentful-migration": "~4.5.9",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "postcss": "^8.4.12",
    "prettier": "~2.4.1",
    "pretty-quick": "~3.1.1",
    "standard-version": "~9.3.2",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.4.4"
  }
}
